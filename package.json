{
  "name": "coinmena",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:1": "react-native run-ios --simulator='iPhone 8'",
    "ios:2": "react-native run-ios --simulator='iPhone 12 Pro Max'",
    "ios:3": "react-native run-ios --simulator='iPhone SE (1st generation)'",
    "start": "react-native start",
    "prettier-fix": "npx prettier src --write --ignore-unknown",
    "eslint-fix": "npx eslint src --fix --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "lint-staged": "13.0.3",
    "react": "18.0.0",
    "react-native": "0.69.1",
    "react-native-safe-area-context": "4.3.1",
    "react-query": "3.39.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "babel-jest": "^26.6.3",
    "eslint": "8.19.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "prettier": "2.7.1",
    "react-test-renderer": "18.0.0",
    "typescript": "4.7.4",
    "husky": "8.0.1"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
